package com.tyss.basicjava;

import java.util.Scanner;

public class Prime {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int x=scanner.nextInt();
		int y=x-1;
		int flag=0;
		while(x!=2){
			if(y%x==0) {
				flag=
			}
			x--;
		}
		if(flag==0) {
			System.out.println("");
		}
		
	}
}
